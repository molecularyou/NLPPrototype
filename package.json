{
  "name": "paper-extraction",
  "version": "0.1.0",
  "author": "matthew mong",
  "description": "electron app for pulling key data from pubmed IDs",
  "private": true,
  "main": "main.js",
  "build": {
    "icon": "public/favicon.png",
    "productName": "Paper Extraction",
    "extraResources": [
      "./extraResources/**"
    ],
    "asarUnpack": [
      "./extraResources/**"
    ],
    "files": [
      "public/**/*",
      "main.js",
      "preload.js"
    ],
    "win": {},
    "linux": {},
    "mac": {}
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear  --port 5555",
    "electron": "wait-on http://localhost:5555 && electron .",
    "electron-dev": "concurrently \"yarn run dev\" \"yarn run electron\"",
    "preelectron-pack": "yarn run build",
    "electron-pack": "electron-builder"
  },
  "dependencies": {
    "electron-serve": "^1.1.0",
    "python-shell": "^3.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "concurrently": "^6.2.1",
    "electron": "^20.0.2",
    "electron-builder": "^22.11.7",
    "rollup": "^2.56.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.12",
    "svelte": "^3.42.1",
    "svelte-dark-mode": "^2.1.0",
    "wait-on": "^6.0.0"
  }
}
